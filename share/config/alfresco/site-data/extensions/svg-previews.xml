<extension>
    <modules>
    
        <!--
            Customization modules for adding i18n labels and importing client-side dependencies for
            the web previewer plugins
        -->
        
        <module>
            <id>svg Viewer</id>
            <customizations>
                <customization>
                    <targetPackageRoot>org.alfresco.components.preview</targetPackageRoot>
                    <sourcePackageRoot>svg.custom.components.preview.svg</sourcePackageRoot>
                </customization>
            </customizations>
            <auto-deploy>true</auto-deploy>
        </module>
        
        <module>
            <id>svg Configuration</id>
            <customizations>
                <customization>
                    <targetPackageRoot>org.alfresco.components.preview</targetPackageRoot>
                    <sourcePackageRoot>svg.custom.components.preview.svg-config</sourcePackageRoot>
                </customization>
            </customizations>
            <auto-deploy>true</auto-deploy>
        </module>
     
    </modules>
</extension>